{
  "tasks": {
    "start": "deno run -A --watch=static/,routes/ dev.ts",
    "add": "deno run -A https://esm.sh/v96 add",
    "update": "deno run -A https://esm.sh/v96 update",
    "remove": "deno run -A https://esm.sh/v96 remove"
  },
  "importMap": "./import_map.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "fmt": {
    "options": {
      "singleQuote": true
    }
  }
}